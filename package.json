{
  "name": "image-webpack-loader",
  "version": "0.1.0",
  "description": "Image optimize loader for webpack",
  "author": "Gleb Mikhee @glebmachine",
  "scripts": {
    "lint": "eslint --fix index.js",
    "lint-watch": "esw --fix --watch index.js",
    "test-webpack": "cd test; webpack"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://www.opensource.org/licenses/mit-license.php"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:tcoopman/image-webpack-loader.git"
  },
  "dependencies": {
    "async-disk-cache": "1.0.7",
    "crypto": "0.0.3",
    "imagemin": "5.2.1",
    "imagemin-mozjpeg": "6.0.0",
    "imagemin-pngquant": "5.0.0",
    "imagemin-svgo": "5.1.0",
    "loader-utils": "0.2.15",
    "q": "1.4.1"
  },
  "devDependencies": {
    "eslint": "2.13.1",
    "eslint-config-airbnb": "9.0.1",
    "eslint-plugin-import": "1.9.2",
    "eslint-watch": "2.1.11",
    "file-loader": "0.9.0",
    "webpack": "1.13.1"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "installedESLint": "true",
    "globals": {
      "ENV": "true"
    },
    "rules": {
      "vars-on-top": 0,
      "no-var": 1,
      "global-require": 0,
      "func-names": 0,
      "prefer-rest-params":1,
      "strict":1,
      "no-param-reassign": 0
    }
  }
}
